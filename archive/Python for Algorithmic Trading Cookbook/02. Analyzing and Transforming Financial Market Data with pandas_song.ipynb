  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "060ec773",
   "metadata": {},
   "outputs": [],
   "source": [
    "s_1 = pd.Series(rnd(), index=seconds)\n",
    "s_2 = pd.Series(rnd(), index=seconds)\n",
    "s_3 = pd.Series(rnd(), index=seconds)"
